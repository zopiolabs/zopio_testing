    "@types/node": "^22.15.21",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "mintlify": "^4.0.589",
    "tsup": "^8.5.0",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "ultracite": "^4.2.5",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18"
